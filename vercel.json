{
  "public": true,
  "functions": {
    "api/main.py": {
      "runtime": "python3.9",
      "maxDuration": 15,
      "excludeFiles": "api/_public"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/main.py"
    },
    {
      "source": "/redoc",
      "destination": "/api/main.py"
    },
    {
      "source": "/docs",
      "destination": "/api/main.py"
    }
  ]
}
